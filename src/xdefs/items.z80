;X-DEF file for items.
;Whether or not you define an X-macro, item ID constants become defined
;on this file's first include.
;Furthermore, if you don't define an X-macro, the X-DEF entries are validated
;to both start at 1 and that all entry IDs are consecutive and gapless.


;NOTE: YOU MUST BUILD THIS WITH CASE-SENSITIVE LABELS. IF YOU DO NOT, YOU'LL
;       GET REDEF ERRORS DUE TO A MACRO NAME MATCHING A LABEL NAME WRT
;       "ITM_START" AND "ITM_END".

#ifndef ITMM_LIST_IDX
#macro itm_start(itemtype)
    #ifndef ITMM_LIST_IDX
        #define ITMM_LIST_IDX 1
        #define ITMM_TYPE_IDX 1
    #endif
    #define ITMM_START_TITLE eval(itemtype,"_START")
    #define ITMM_START_NAME concat(itemtype,"_START")
    #define ITMM_TYPEID_NAME concat(itemtype,"_TYPEID")
    ITMM_START_NAME .equ ITMM_LIST_IDX
    ITMM_TYPEID_NAME .equ ITMM_TYPE_IDX
    #define ITMM_TYPE_IDX eval(ITMM_TYPE_IDX+1)
#endmacro

#macro itm_def(itemtype, itemname)
    #define ITMM_DEF_TITLE eval(itemtype,"_",itemname)
    #define ITMM_DEF_NAME concat(itemtype,"_",itemname)
    ITMM_DEF_NAME .equ ITMM_LIST_IDX
    #define ITMM_LIST_IDX eval(ITMM_LIST_IDX+1)
#endmacro

#macro itm_end(itemtype)
    #define ITMM_END_TITLE eval(itemtype,"_END")
    #define ITMM_END_NAME concat(itemtype,"_END")
    ITMM_END_NAME .equ ITMM_LIST_IDX
#endmacro
