; Make sure you have a section in your X-macro to stop generating data
; when you want to stop tabulating. (e.g. for equipment-only entries, only
; parse entries where YOUR_ID_COUNT_VAR < EQUIPMENT_END. Yes, EQUIPMENT_END
; is an actual constant. Use it. See the above macros for what gets generated)
;

#ifndef X
    #ifdef X_IDCOUNT
        #undef IX_IDCOUNT
    #endif
    #macro X(id,nm,gc,at,de,wi,ag,hi,cr,f1,f2,f3)
        #ifndef X_IDCOUNT
            #define X_IDCOUNT eval(1)
        #endif
        #if (X_IDCOUNT != id)
            .error "Item validation failed at item code ",id,", expecting ",X_IDCOUNT
        #endif
        #define X_IDCOUNT eval(X_IDCOUNT+1)
    #endmacro
#endif


